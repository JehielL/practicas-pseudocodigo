Algoritmo funcionesStringConcatenar
	
	Imprimir "Escribe tu nombre"
	Leer firstName
	
	Imprimir "Escribe tu apellido"
	Leer lastName
	space <- " "
	
	
	fullName <- Concatenar(firstName,space)
	fullLastName <- Concatenar(fullName,lastName)
	
	
	Imprimir  "Tu nombre completo es :" fullLastName
FinAlgoritmo
